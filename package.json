{
    "name": "@egomobile/next-utils",
    "version": "0.5.0",
    "description": "Handy utils and extensions for Next.js",
    "main": "lib/index.js",
    "engines": {
        "node": ">=18.0.0"
    },
    "scripts": {
        "build": "del ./lib && tsc",
        "docs:generate": "del ./docs && typedoc --out ./docs ./src",
        "lint": "eslint -c .eslintrc.js --ext .ts src",
        "lint:fix": "eslint --fix -c .eslintrc.js --ext .ts src",
        "sandbox": "nodemon --watch './sandbox/index.ts' ./sandbox/index.ts",
        "test": "exit 0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/egomobile/next-utils"
    },
    "author": "Next.e.GO Mobile SE, Aachen, Germany",
    "license": "LGPL-3.0",
    "files": [
        "CHANGELOG.md",
        "LICENSE",
        "lib",
        "!lib/**/__tests__/**/*",
        "!lib/**/*.map",
        "!lib/**/setupTests.*",
        "README.md"
    ],
    "keywords": [
        "nextjs",
        "next js",
        "next.js",
        "next",
        "js",
        "javascript",
        "ts",
        "typescript",
        "util",
        "utils",
        "helper",
        "helpers",
        "class",
        "classes",
        "function",
        "functions"
    ],
    "dependencies": {
        "@egomobile/api-utils": "^1.2.0",
        "@egomobile/node-utils": "^0.3.0",
        "@egomobile/nodelike-utils": "^0.2.2",
        "@types/node": "18.11.9"
    },
    "devDependencies": {
        "@egomobile/http-server": "^0.63.1",
        "@egomobile/tsconfig": "^5.0.0",
        "del-cli": "5.0.0",
        "eslint": "8.46.0",
        "eslint-config-ego": "^0.17.1",
        "eslint-plugin-unicorn": "48.0.1",
        "next": "13.0.0",
        "nodemon": "3.0.1",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "ts-node": "10.9.1",
        "typedoc": "0.24.8",
        "typescript": "4.6.4"
    },
    "peerDependencies": {
        "@egomobile/http-server": ">=0.63.1",
        "next": ">=13.0.0"
    }
}